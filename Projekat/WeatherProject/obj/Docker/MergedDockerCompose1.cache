services:
  weathersiberiandata:
    build:
      context: D:\PROJEKAT SOA\SOA\Projekat\WeatherProject
      dockerfile: WeatherSiberianData/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WeatherSiberianData
      target: base
    container_name: WeatherSiberianData
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: weathersiberiandata:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/WeatherSiberianData.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - D:\PROJEKAT SOA\SOA\Projekat\WeatherProject\WeatherSiberianData:/app:rw
    - C:\Users\arsic\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\arsic\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\arsic\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\arsic\.nuget\packages:/root/.nuget/packages:ro
    - D:\PROJEKAT SOA\SOA\Projekat\WeatherProject:/src:rw
  weathersiberiansensor:
    build:
      context: D:\PROJEKAT SOA\SOA\Projekat\WeatherProject
      dockerfile: WeatherSiberianSensor/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WeatherSiberianSensor
      target: base
    container_name: WeatherSiberianSensor
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: weathersiberiansensor:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/WeatherSiberianSensor.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - D:\PROJEKAT SOA\SOA\Projekat\WeatherProject\WeatherSiberianSensor:/app:rw
    - C:\Users\arsic\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\arsic\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\arsic\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\arsic\.nuget\packages:/root/.nuget/packages:ro
    - D:\PROJEKAT SOA\SOA\Projekat\WeatherProject:/src:rw
version: '3.4'